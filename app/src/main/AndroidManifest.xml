<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tictactoe">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconimage"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".rules"></activity>
        <activity
            android:name=".score"
            android:configChanges="screenLayout"></activity>
        <activity android:name=".finalpage" />
        <activity android:name=".game5x5" />
        <activity android:name=".gamemode" />
        <activity android:name=".game4X4" />
        <activity android:name=".game3x3" />
        <activity android:name=".dual" />
        <activity android:name=".solo" />
        <activity
            android:name=".page1"
            android:configChanges="screenLayout" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>